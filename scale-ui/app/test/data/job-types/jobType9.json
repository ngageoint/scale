{
  "id": 9,
  "name": "wv2-ortho",
  "title": "WorldView-2 Rigorous Orthorectification",
  "version": "2.0",
  "description": "Applies Orthorectification to WorldView-2 data",
  "category": "worldview",
  "is_operational": true,
  "icon_code": "f219",
  "docker_privileged": false,
  "docker_image": "10.4.4.10:5000/wv2-ortho:2.0",
  "priority": 1,
  "timeout": 30,
  "max_tries": 3,
  "cpus_required": 4.0,
  "mem_required": 4096.0,
  "shared_mem_required": 512.0,
  "disk_out_const_required": 2048.0,
  "disk_out_mult_required": 0.0,
  "interface": {
    "output_data": [{
      "media_type": "image/tiff",
      "required": true,
      "type": "file",
      "name": "wv2-ortho-out"
    }],
    "shared_resources": [],
    "command_arguments": "${infile} ${job_output_dir}",
    "input_data": [{
      "media_types": ["application/octet-stream"],
      "required": true,
      "type": "file",
      "name": "infile"
    }],
    "version": "2.0",
    "command": "./wv2_ortho.py"
  },
  "error_mapping": {}
}
