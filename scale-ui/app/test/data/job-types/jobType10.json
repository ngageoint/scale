{
  "id": 10,
  "name": "export-shp",
  "title": "Export Shapefile",
  "version": "2.0",
  "description": "Creates shapefile (.shp) from raster data",
  "category": "export",
  "is_operational": true,
  "icon_code": "f06e",
  "docker_privileged": false,
  "docker_image": "10.4.4.10:5000/export-shp:2.0",
  "priority": 1,
  "timeout": 30,
  "max_tries": 3,
  "cpus_required": 4.0,
  "mem_required": 4096.0,
  "shared_mem_required": 256.0,
  "disk_out_const_required": 2048.0,
  "disk_out_mult_required": 0.0,
  "interface": {
    "output_data": [{
      "media_type": "application/xml",
      "required": true,
      "type": "file",
      "name": "shapefile"
    }],
    "shared_resources": [],
    "command_arguments": "${infile} ${job_output_dir}",
    "input_data": [{
      "media_types": ["application/octet-stream"],
      "required": true,
      "type": "file",
      "name": "infile"
    }],
    "version": "2.0",
    "command": "./export_shapefile.sh"
  },
  "error_mapping": {}
}
